<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.default" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/textcolor</item>
        <item name="colorPrimaryVariant">@color/textcolor</item>
        <item name="colorOnPrimary">@color/textcolor</item>
        <item name="colorPrimaryDark">@color/textcolor</item>

        <!-- Status bar color. -->
       <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.default.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="BusinessBottomNavigationViewActive" parent="Widget.Design.BottomNavigationView">
        <item name="android:textColor">@color/app</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="fontFamily">@font/medium</item>
    </style>



    <style name="BottomNavigationViewInActive" parent="Widget.Design.BottomNavigationView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="fontFamily">@font/medium</item>
    </style>


</resources>